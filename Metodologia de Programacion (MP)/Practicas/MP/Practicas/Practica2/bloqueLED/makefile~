INCLUDE = include
LIB = lib
OBJ = obj
SRC = src
BIN = bin

all: $(BIN)/bloqueLedexe

$(BIN)/bloqueLedexe : $(OBJ)/Demo2.o
	g++ -o $(BIN)/bloqueLedexe $(OBJ)/Demo2.o

$(OBJ)/test.o : $(SRC)/test.cpp $(INCLUDE)/bloqueLed.h
	g++ -c -I$(INCLUDE) -o $(OBJ)/test.o $(SRC)/test.cpp

$(OBJ)/bloqueLed.o : $(SRC)/bloqueLed.cpp $(INCLUDE)/bloqueLed.h
	g++ -c -I$(INCLUDE) -o $(OBJ)/bloqueLed.o $(SRC)/bloqueLed.cpp


