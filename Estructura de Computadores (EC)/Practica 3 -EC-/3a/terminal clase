josele@josele-VB:~/Escritorio/Practica 3 -EC-/3a/3a$ cat suma_03_SC_c.c
int suma(int* array, int len)
{
	int i, res=0;
	for (i=0; i<len; i++)
		res += array[i];
	return res;
}
